# 服务器配置
PORT=3001
HOST=0.0.0.0
NODE_ENV=development

# 数据库配置
DATABASE_URL="file:./dev.db"

# JWT 配置
JWT_SECRET=autoFetchExpress
JWT_EXPIRES_IN=7d

# 加密配置 (用于加密 Cookie 等敏感信息)
ENCRYPTION_SECRET=autoFetchExpress

# CORS 配置
CORS_ORIGIN=*

# 日志级别 (error | warn | info | http | debug)
LOG_LEVEL=info

# 任务调度配置
SCHEDULER_ENABLED=true
SCHEDULER_TIMEZONE=Asia/Shanghai
